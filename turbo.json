{
	"$schema": "https://turborepo.org/schema.json",
	"tasks": {
			"clean": {
					"cache": false
			},
			"watch": {
				"cache": false,
				"persistent": true,
				"outputs": [
						"dist/**/*",
						"build/**/*",
						".next/**/*",
						"public/**/*",
						"static/examples_js/**/*",
						"static/examples_ts/**/*"
				]
			},
			"build": {
					"dependsOn": ["^build"],
					"outputs": [
							"dist/**/*",
							"build/**/*",
							".next/**/*",
							"public/**/*",
							"static/examples_js/**/*",
							"static/examples_ts/**/*"
					]
			},
			"check": {
					"dependsOn": ["^build"],
					"outputs": []
			},
			"test": {
					"dependsOn": ["build", "^build"],
					"outputs": ["coverage/**/*"]
			},
			"ci": {
					"dependsOn": ["build", "check", "test"]
			},
			"release": {
					"cache": false
			}
	}
}