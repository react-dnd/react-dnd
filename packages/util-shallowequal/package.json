{
  "name": "@react-dnd/shallowequal",
  "version": "4.0.0",
  "description": "Like lodash isEqualWith but for shallow equal.",
  "keywords": [
    "shallowequal",
    "shallow",
    "equal",
    "isequal",
    "compare",
    "isequalwith"
  ],
  "license": "MIT",
  "author": {
    "name": "Alberto Leal",
    "email": "mailforalberto@gmail.com",
    "url": "https://github.com/dashed"
  },
