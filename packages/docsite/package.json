{
  "name": "react-dnd-documentation",
  "description": "React DnD Docsite",
  "version": "14.0.3",
  "private": true,
  "license": "MIT",
  "dependencies": {
    "debug": "^4.2.0",
    "gatsby": "~3.14.0",
    "gatsby-plugin-manifest": "^3.14.0",
    "gatsby-plugin-react-helmet": "^4.14.0",
    "gatsby-plugin-sharp": "^3.14.0",
    "gatsby-plugin-styled-components": "^4.14.0",
    "gatsby-plugin-typography": "^3.14.0",
    "gatsby-remark-autolink-headers": "^4.11.0",
    "gatsby-remark-copy-linked-files": "^4.11.0",
    "gatsby-remark-images": "^5.11.0",
    "gatsby-remark-prismjs": "^5.11.0",
    "gatsby-source-filesystem": "^3.14.0",
    "gatsby-transformer-remark": "^4.11.0",
    "gatsby-transformer-sharp": "^3.14.0",
    "lodash": "^4.17.21",
    "prismjs": "^1.25.0",
    "query-string": "^6.14.1",
    "react": "^17.0.2",
    "react-dnd": "workspace:packages/react-dnd",
    "react-dnd-examples-decorators": "workspace:packages/examples-decorators",
    "react-dnd-examples-hooks": "workspace:packages/examples-hooks",
    "react-dnd-html5-backend": "workspace:packages/backend-html5",
    "react-dnd-test-backend": "workspace:packages/backend-test",
    "react-dnd-touch-backend": "workspace:packages/backend-touch",
    "react-dom": "^17.0.2",
    "react-error-overlay": "^6.0.9",
    "react-helmet": "^6.1.0",
    "react-is": "17.0.2",
    "react-tabs": "^3.2.2",
    "react-typography": "^0.16.20",
    "regenerator-runtime": "^0.13.9",
    "rehype-react": "^6.2.1",
    "shallow-compare": "^1.2.2",
    "styled-components": "^5.3.1",
    "typography": "^0.16.21",
    "typography-theme-github": "^0.16.19"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/react-dnd/react-dnd.git",
    "directory": "packages/docsite"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@types/query-string": "^6.3.0",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/react-helmet": "^6.1.2",
    "@types/react-tabs": "^2.3.3",
    "@types/rehype-react": "^4.0.0",
    "@types/styled-components": "^5.1.14",
    "babel-plugin-styled-components": "^1.13.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "prop-types": "^15.7.2",
    "react-refresh": "^0.9.0",
    "rimraf": "^3.0.2",
    "shx": "^0.3.3"
  },
  "scripts": {
    "clean_examples": "rimraf static/examples_decorators_js static/examples_decorators_ts static/examples_hooks_js static/examples_hooks_ts",
    "clean_docsite": "rimraf .cache public apidocs",
    "clean": "run-s clean_examples clean_docsite",
    "copy_examples:js_hooks": "shx cp -r ../examples-hooks/dist/docs static/examples_hooks_js",
    "copy_examples:ts_hooks": "shx cp -r ../examples-hooks/src static/examples_hooks_ts",
    "copy_examples:js_decorators": "shx cp -r ../examples-decorators/dist/docs static/examples_decorators_js",
    "copy_examples:ts_decorators": "shx cp -r ../examples-decorators/src static/examples_decorators_ts",
    "copy_examples": "run-s 'copy_examples:*'",
    "prune_buildinfo": "rimraf static/examples_hooks_js/tsconfig.docs.tsbuildinfo static/examples_decorators_js/tsconfig.docs.tsbuildinfo",
    "format_examples": "prettier 'static/examples_*/**/*.(js|ts|tsx)' --use-tabs=false --write",
    "write_example_boilerplate": "node write_sample_boilerplate.js",
    "echo_finished_examples": "shx echo 'finished processing examples'",
    "process_examples": "run-s clean_examples copy_examples prune_buildinfo format_examples write_example_boilerplate echo_finished_examples",
    "build:gatsby": "gatsby build --prefix-paths",
    "start:docsite": "gatsby develop -H 0.0.0.0",
    "build:docsite": "run-s process_examples build:gatsby"
  }
}
