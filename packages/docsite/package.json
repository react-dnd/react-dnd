{
  "name": "react-dnd-documentation",
  "description": "React DnD Docsite",
  "version": "15.1.2",
  "private": true,
  "license": "MIT",
  "dependencies": {
    "debug": "^4.3.4",
    "gatsby": "~4.12.1",
    "gatsby-plugin-manifest": "^4.12.1",
    "gatsby-plugin-react-helmet": "^5.12.1",
    "gatsby-plugin-sharp": "^4.12.1",
    "gatsby-plugin-styled-components": "^5.12.1",
    "gatsby-plugin-typography": "^4.12.1",
    "gatsby-remark-autolink-headers": "^5.12.1",
    "gatsby-remark-copy-linked-files": "^5.12.1",
    "gatsby-remark-images": "^6.12.1",
    "gatsby-remark-prismjs": "^6.12.1",
    "gatsby-source-filesystem": "^4.12.1",
    "gatsby-transformer-remark": "^5.12.1",
    "gatsby-transformer-sharp": "^4.12.1",
    "lodash": "^4.17.21",
    "prismjs": "^1.29.0",
    "query-string": "^6.14.1",
    "react": "^18.2.0",
    "react-dnd": "workspace:^",
    "react-dnd-examples": "workspace:^",
    "react-dnd-html5-backend": "workspace:^",
    "react-dnd-test-backend": "workspace:^",
    "react-dnd-touch-backend": "workspace:^",
    "react-dom": "^18.2.0",
    "react-error-overlay": "^6.0.11",
    "react-helmet": "^6.1.0",
    "react-is": "18.2.0",
    "react-tabs": "^5.2.0",
    "react-typography": "^0.16.23",
    "rehype-react": "^6.2.1",
    "shallow-compare": "^1.2.2",
    "styled-components": "^5.3.6",
    "typography": "^0.16.21",
    "typography-theme-github": "^0.16.19"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/react-dnd/react-dnd.git",
    "directory": "packages/docsite"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-transform-react-jsx": "^7.20.7",
    "@react-dnd/eslint-config": "workspace:^",
    "@types/query-string": "^6.3.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/react-helmet": "^6.1.6",
    "@types/react-tabs": "^5.0.5",
    "@types/styled-components": "^5.1.26",
    "babel-plugin-styled-components": "^2.0.7",
    "eslint": "^8.32.0",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.8.1",
    "react-refresh": "^0.14.0",
    "rome": "^11.0.0",
    "shx": "^0.3.4"
  },
  "scripts": {
    "_clean_examples": "shx rm -rf static/examples_js static/examples_ts",
    "_clean_docsite": "shx rm -rf .cache public apidocs",
    "_copy_js_examples": "shx cp -r ../examples/dist/docs static/examples_js",
    "_copy_ts_examples": "shx cp -r ../examples/src static/examples_ts",
    "_copy_examples": "run-p _copy_js_examples _copy_ts_examples",
    "_prune_buildinfo": "shx rm -rf static/examples_js/tsconfig.docs.tsbuildinfo",
    "_format_examples": "rome format static/examples_js static/examples_ts --write",
    "_write_example_boilerplate": "node write_sample_boilerplate.mjs",
    "_echo_finished": "shx echo 'finished processing examples'",
    "_process_examples": "run-s _clean_examples _copy_examples _prune_buildinfo _format_examples _write_example_boilerplate _echo_finished",
    "_gatsby_build": "gatsby build --prefix-paths",
    "build:docsite": "yarn build",
    "start:docsite": "yarn start",
    "clean": "run-s _clean_examples _clean_docsite",
    "build": "run-s _process_examples _gatsby_build",
    "start": "gatsby develop -H 0.0.0.0",
    "check": "eslint src --ext .js,.jsx,.mjs,.cjs,.ts,.tsx,.cts,.mts --resolve-plugins-relative-to ../eslint-config"
  }
}
