{
  "name": "@react-dnd/invariant",
  "version": "5.0.0",
  "description": "invariantx",
  "keywords": [
    "test",
    "invariant"
  ],
  "license": "MIT",
  "author": "Andres Suarez <zertosh@gmail.com>",
  "repository": "https://github.com/react-dnd/react-dnd",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "scripts": {
    "clean": "shx rm -rf dist/",
    "build_types": "tsc -b .",
    "build_esm": "swc -C module.type=es6 -d dist/esm src/",
    "build_commonjs": "swc -C module.type=commonjs -d dist/cjs src/",
    "add_pkgjson": "node ../../scripts/createDistPackageJson.mjs",
    "build": "run-s clean build_types build_esm build_commonjs add_pkgjson"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.23",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.16",
    "npm-run-all": "^4.1.5",
    "shx": "^0.3.4",
    "typescript": "~4.8.4"
  }
}
