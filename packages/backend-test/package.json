{
  "name": "react-dnd-test-backend",
  "version": "15.1.2",
  "description": "A mock backend for testing React DnD apps",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/react-dnd/react-dnd.git"
  },
  "scripts": {
    "clean": "shx rm -rf dist/",
    "build_types": "tsc -b .",
    "build_esm": "swc -C module.type=es6 -d dist/esm src/",
    "build_cjs": "swc -C module.type=commonjs -d dist/cjs src/",
    "build": "run-s build_types build_esm build_cjs"
  },
  "dependencies": {
    "dnd-core": "workspace:packages/dnd-core"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.161",
    "npm-run-all": "^4.1.5",
    "shx": "^0.3.4",
    "typescript": "^4.6.3"
  }
}
