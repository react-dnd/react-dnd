{
  "name": "dnd-core",
  "version": "16.0.1",
  "description": "Drag and drop sans the GUI",
  "license": "MIT",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "clean": "shx rm -rf dist/",
    "build_types": "tsc -b .",
    "build_esm": "swc -C module.type=es6 -d dist/esm src/",
    "build_commonjs": "swc -C module.type=commonjs -d dist/cjs src/",
    "add_pkgjson": "node ../../scripts/createDistPackageJson.mjs",
    "build": "run-s clean build_types build_esm build_commonjs add_pkgjson"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/react-dnd/react-dnd.git"
  },
  "dependencies": {
    "@react-dnd/asap": "workspace:^",
    "@react-dnd/invariant": "workspace:^",
    "redux": "^4.2.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.23",
    "@types/jest": "^29.2.4",
    "@types/setimmediate": "^1.0.2",
    "jest-mock": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "react-dnd-test-backend": "portal:../backend-test",
    "setimmediate": "^1.0.5",
    "shx": "^0.3.4",
    "typescript": "~4.8.4"
  }
}
