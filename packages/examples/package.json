{
  "name": "react-dnd-examples",
  "version": "16.0.0",
  "private": true,
  "description": "Drag and Drop for React",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/react-dnd/react-dnd.git"
  },
  "license": "MIT",
  "scripts": {
    "clean": "shx rm -rf dist/",
    "build_types": "tsc -b .",
    "build_esm": "swc -C module.type=es6 -d dist src/",
    "build_docs": "tsc -b tsconfig.docs.json",
    "unjsxify": "yarn node ../../scripts/unjsxify.mjs ./dist/docs",
    "build": "run-s build_types build_esm build_docs unjsxify"
  },
  "dependencies": {
    "dnd-core": "workspace:^",
    "faker": "^5.5.3",
    "immutability-helper": "^3.1.1",
    "lodash": "^4.17.21",
    "react-dnd": "workspace:^",
    "react-dnd-html5-backend": "workspace:^",
    "react-frame-component": "^5.2.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.168",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.25",
    "@types/react": "^18.0.5",
    "jest-mock": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "react": "^18.0.0",
    "react-dnd-test-utils": "workspace:^",
    "shx": "^0.3.4",
    "typescript": "^4.6.3"
  },
  "peerDependencies": {
    "@types/node": ">= 12",
    "@types/react": ">= 16",
    "prop-types": ">= 15",
    "react": ">= 16.14",
    "react-dnd": ">11.1.3"
  },
  "peerDependenciesMeta": {
    "@types/node": {
      "optional": true
    },
    "@types/react": {
      "optional": true
    },
    "react-dnd": {
      "optional": false
    }
  }
}
